<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
         http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
         http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="blog-web" logger="slf4j"/>
    <dubbo:registry protocol="zookeeper" address="${dubbo.registry.address}" register="true" timeout="100000"/>
    <dubbo:protocol name="dubbo" port="${dubbo.protocol.port}"/>

    <dubbo:reference id="merInfoService" interface="com.jiupai.platform.merchant.api.IMerInfoService" version="0.0.1" loadbalance="roundrobin" cluster="failfast" timeout="50000"/>
    <dubbo:monitor protocol="registry" />
</beans>
